{
  "name": "naxtap-marketplace",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "packageManager": "npm@10.9.0",
  "scripts": {
    "start": "expo start",
    "start:dev": "expo start --dev-client",
    "start:web": "node scripts/start-web-fixed.js",
    "start:web:direct": "cross-env EXPO_ROUTER_APP_ROOT=./app EXPO_ROUTER_IMPORT_MODE=async expo start --web --clear",
    "start:web:dev": "DEBUG=expo* expo start --web",
    "build:web": "expo export --platform web && node scripts/fix-web-index-module.js",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "test": "jest",
    "preview": "expo export --platform web && node scripts/fix-web-index-module.js && npx serve dist",
    "dev": "npm run start",
    "prod": "npm run build:web",
    "server": "node server.js",
    "build:backend": "tsc -p backend/tsconfig.backend.json",
    "start:backend": "node backend/dist/server.js",
    "server:ts": "npm run build:backend && npm run start:backend",
    "deploy:backend": "npm run build:backend && npx prisma generate && npx prisma migrate deploy && npm run start:backend",
    "deploy:web": "npm run build:web && npx serve dist -s -p ${PORT:-8080}"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.5",
    "@expo/vector-icons": "^14.1.0",
    "@hono/node-server": "^1.19.5",
    "@hono/trpc-server": "^0.4.0",
    "@nkzw/create-context-hook": "^1.1.0",
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/netinfo": "11.4.1",
    "@react-navigation/native": "^7.1.6",
    "@tanstack/react-query": "^5.90.2",
    "@trpc/client": "^11.6.0",
    "@trpc/react-query": "^11.6.0",
    "@trpc/server": "^11.6.0",
    "@types/jsonwebtoken": "^9.0.10",
    "cookie": "^1.0.2",
    "dotenv": "^17.2.3",
    "expo": "^53.0.4",
    "expo-av": "~15.1.7",
    "expo-blur": "~14.1.4",
    "expo-camera": "~16.1.11",
    "expo-clipboard": "~7.1.5",
    "expo-constants": "~17.1.4",
    "expo-device": "~7.1.4",
    "expo-document-picker": "~13.1.6",
    "expo-font": "~13.3.0",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.1",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "~14.1.4",
    "expo-linking": "~7.1.4",
    "expo-location": "~18.1.4",
    "expo-mail-composer": "~14.1.6",
    "expo-notifications": "~0.31.4",
    "expo-router": "~5.1.7",
    "expo-sharing": "~13.1.5",
    "expo-splash-screen": "~0.30.7",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.4",
    "expo-system-ui": "~5.0.6",
    "expo-web-browser": "~14.2.0",
    "hono": "^4.9.12",
    "jose": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react-native": "0.545.0",
    "nativewind": "^4.1.23",
    "pg": "^8.16.3",
    "prisma": "^7.1.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.6",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "^0.20.0",
    "superjson": "^2.2.2",
    "zod": "^4.1.11",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@expo/ngrok": "^4.1.0",
    "@types/jest": "^29.5.14",
    "@types/pg": "^8.16.0",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-plugin-react-hooks": "^7.0.0",
    "express": "^5.2.1",
    "http-proxy-middleware": "^3.0.5",
    "jest": "^29.7.0",
    "jest-expo": "^53.0.13",
    "pm2": "^5.3.0",
    "pngjs": "^7.0.0",
    "serve": "^14.2.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3"
  },
  "private": true,
  "prisma": {
    "seed": "ts-node backend/seed.ts"
  }
}
